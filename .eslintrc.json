{
  "extends": ["airbnb","plugin:prettier/recommended"],
  "env": {
    "jest": true,
    "browser": true
  },
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}